{
  "name": "song-identifier-app",
  "version": "1.0.0",
  "description": "Song identifier from video URLs - TikTok, Instagram, YouTube",
  "scripts": {
    "install:server": "cd server && npm install",
    "install:client": "cd client && npm install",
    "build:server": "cd server && npm run build",
    "build:client": "cd client && npm run build",
    "start:server": "cd server && npm start",
    "start:client": "cd client && npm run preview -- --host 0.0.0.0",
    "install": "npm run install:server",
    "build": "npm run build:server",
    "start": "npm run start:server"
  },
  "keywords": ["song-identifier", "music-recognition", "tiktok", "instagram", "youtube"],
